@model CmsClientApp.Models.Schedule
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
<div>
    <label asp-for="AppointmentTime" class="control-label"></label>
    <select asp-for="AppointmentTime" class="form-control">
        @foreach (var item in ViewBag.res)
        {
<option>@item:00</option>
}
    </select>
    <div class="form-group">
        <input type="submit" value="Book Appointment" class="btn btn-success" />
    </div>
</div>